   <properties>
    <property name="dialog">[
	{
		&quot;name&quot;: &quot;&quot;,
		&quot;text&quot;: &quot;The gate is locked. A sign reads 'WINNERS ONLY'.&quot;,
		&quot;options&quot;:
			[
				{
					&quot;name&quot;: &quot;Ask for the gate to be opened.&quot;,
					&quot;action&quot;: [ { &quot;deleteMapObject&quot;: -1 }, { &quot;deleteMapObject&quot;: 117 } ],
					&quot;condition&quot;: [
						{
							&quot;getMapFlag&quot;: {
